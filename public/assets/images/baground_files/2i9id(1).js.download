function slickAutoResponsive(slider, max, size, round=false){
if(!max){
max=5;
}
if(!size){
size=380;
}
var width=slider.width()||0;
var count;
if(round){
count=Math.floor(width / size);
}else{
count=Math.ceil(width / size);
}
if(count < 1) count=1;
if(count > max) count=max;
return count;
}
